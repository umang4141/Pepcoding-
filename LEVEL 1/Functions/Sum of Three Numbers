import java.util.Scanner;
public class javafun {
    public static int sum(int a,int b,int c)
    {
        int ans=a+b+c;
        return ans;
    }
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        int c = sc.nextInt();
        int ans=sum(a,b,c);

        System.out.println(ans);
    }
}
